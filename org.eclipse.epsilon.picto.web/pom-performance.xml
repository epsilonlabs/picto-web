	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<forkCount>1</forkCount>
					<reuseForks>true</reuseForks>
					<argLine>-Xmx24576m -Xms1024m</argLine>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
				<configuration>
					<mainClass>org.eclipse.epsilon.picto.web.test.JavaPerformanceProcess</mainClass>
					<excludes>
						<exclude>
							<groupId>org.projectlombok</groupId>
							<artifactId>lombok</artifactId>
						</exclude>
					</excludes>

					<!-- PICTO WEB -->
					<finalName>performance</finalName>
					<!--<appendAssemblyId>false</appendAssemblyId> -->
					<outputDirectory>./</outputDirectory>
					<!-- <archive> <manifest> <addClasspath>true</addClasspath> <mainClass>org.eclipse.epsilon.picto.web.test.Dummy</mainClass> 
						</manifest> </archive> <descriptorRefs> <descriptorRef>jar-with-dependencies</descriptorRef> 
						</descriptorRefs> -->
					<!-- PICTO WEB -->

				</configuration>
			</plugin>
		</plugins>
	</build>
	<url>https://github.com/epsilonlabs/picto-web</url>
	<artifactId>picto-web</artifactId>
	<organization>
		<name>EpsilonLabs, University of York</name>
		<url>https://github.com/epsilonlabs/</url>
	</organization>
</project>